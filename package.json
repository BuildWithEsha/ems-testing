{
  "name": "employee-management-system",
  "version": "1.0.0",
  "description": "Employee Management System with Task Management",
  "main": "index.js",
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "optimize:db": "node run_optimizations.js",
    "optimize:all": "npm run optimize:db && npm run build",
    "check-teamlogger-keys": "node scripts/check-teamlogger-api-keys.js",
    "test:teamlogger-report": "node scripts/test-teamlogger-employee-summary.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.4",
    "express": "^4.18.2",
    "lucide-react": "^0.263.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
